<h1>Edit Article</h1>
<%= form_for :article, url: article_path(@article), method: :put do |f| %>
    <p>
        <%= f.label :title %>
    </p>
    <p>
        <%= f.text_field :title %>
    </p>
    <p>
        <%= f.label :text %>
    </p>
    <p>
        <%= f.text_area :text %>
    </p>
    <p>
        <%= f.submit %>
    </p>
<% end %>